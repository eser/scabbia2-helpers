{
    "name": "eserozvataf/scabbia2-helpers",
    "description": "Scabbia2 Helpers Component",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "keywords": [
        "eserozvataf",
        "scabbia",
        "scabbia2",
        "scabbiafw",
        "helpers"
    ],
    "homepage": "https://github.com/eserozvataf/scabbia2",
    "license": [
        "Apache-2.0"
    ],
    "authors": [
        {
            "name": "Eser Ozvataf",
            "homepage": "http://eser.ozvataf.com/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/eserozvataf/scabbia2-helpers/blob/master/contributors.md"
        }
    ],
    "support": {
        "issues": "https://github.com/eserozvataf/scabbia2-helpers/issues",
        "source": "https://github.com/eserozvataf/scabbia2-helpers"
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "eserozvataf/scabbia2-tasks": "*",
        "eserozvataf/scabbia2-testing": "*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "suggest": {
        "ext-fileinfo": "Required for mimetype resolution",
        "ext-mbstring": "Required for multibyte string manipulation"
    },
    "autoload": {
        "psr-4": {
            "Scabbia\\Helpers\\": [ "src/Helpers/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scabbia\\Tests\\Helpers\\": [ "tests/Helpers/" ]
        }
    }
}
