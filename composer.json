{
    "name": "scabbiafw/scabbia2-helpers",
    "description": "Scabbia2 Helpers Component",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "keywords": [
        "larukedi",
        "scabbia",
        "scabbia2",
        "scabbiafw",
        "helpers"
    ],
    "homepage": "http://www.scabbiafw.com/",
    "license": [
        "Apache-2.0"
    ],
    "authors": [
        {
            "name": "Scabbia Framework Organization",
            "homepage": "http://scabbiafw.com/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/scabbiafw/scabbia2-helpers/blob/master/contributors.md"
        }
    ],
    "support": {
        "issues": "https://github.com/scabbiafw/scabbia2-helpers/issues",
        "source": "https://github.com/scabbiafw/scabbia2-helpers"
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "scabbiafw/scabbia2-tasks": "*",
        "scabbiafw/scabbia2-testing": "*"
    },
    "suggest": {
        "ext-fileinfo": "Required for mimetype resolution",
        "ext-mbstring": "Required for multibyte string manipulation"
    },
    "autoload": {
        "psr-4": {
            "Scabbia\\Helpers\\": [ "src/Helpers/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scabbia\\Tests\\Helpers\\": [ "tests/Helpers/" ]
        }
    }
}
